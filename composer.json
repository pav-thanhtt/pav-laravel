{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "bensampo/laravel-enum": "^1.26",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "guidocella/eloquent-insert-on-duplicate-key": "^2.2",
        "kyslik/column-sortable": "^6.2",
        "laravel/framework": "^6.2",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/tinker": "^1.0",
        "laravel/ui": "^1.0",
        "laravelcollective/html": "^6.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "milon/barcode": "^6.0",
        "monolog/monolog": "1.16",
        "predis/predis": "^1.1",
        "s-ichikawa/laravel-sendgrid-driver": "^2.0",
        "stechstudio/laravel-zipstream": "^2.4",
        "tucker-eric/eloquentfilter": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpmd/phpmd": "@stable",
        "phpro/grumphp": "^0.15.2",
        "phpunit/phpunit": "^8.0",
        "sebastian/phpcpd": "^4.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "PCB\\TimeZones\\": "packages/pcb/timezones/src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "c": [
            "rm -rf ./bootstrap/cache/*.php",
            "composer dump-autoload",
            "php artisan clear-compiled",
            "php artisan optimize",
            "php artisan config:cache"
        ]
    }
}